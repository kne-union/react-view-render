{
  "id": "Avatar",
  "name": "头像上传",
  "property": {
    "label": {
      "input-type": "$input",
      "label": "label"
    },
    "name": {
      "input-type": "$input",
      "label": "name"
    },
    "rule": {
      "input-type": "$input",
      "label": "验证规则"
    },
    "interceptor": {
      "input-type": "$input",
      "label": "拦截器"
    },
    "editor": {
      "input-type": "$object",
      "label": "裁剪",
      "input-options": {
        "open": {
          "input-type": "$switch",
          "label": "是否开启裁剪"
        },
        "width": {
          "input-type": "$number",
          "label": "裁剪宽度"
        },
        "height": {
          "input-type": "$number",
          "label": "裁剪高度"
        },
        "borderRadius": {
          "input-type": "$number",
          "label": "圆角"
        },
        "text": {
          "input-type": "$input",
          "label": "确定按钮文案"
        }
      }
    }
  },
  "children": {
    "allow": [],
    "dependence": [
      "Form"
    ]
  },
  "defaultProps": {
    "label": "头像",
    "name": "$id",
    "editor": {
      "open": true
    }
  }
}
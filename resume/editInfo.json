{
  "functions": {
    "$changeInfoState": "functions.$setEditInfoState(true);",
    "$changeEditState": "functions.$setEditInfoState(false);"
  },
  "data": [
    {
      "id": "State-0",
      "component": "State",
      "props": {
        "initValue": true,
        "setStateName": "$setEditInfoState"
      },
      "children": [
        {
          "id": "Condition-1",
          "component": "Condition",
          "props": {
            "resource": "$item"
          },
          "children": [
            {
              "id": "Component-2",
              "component": "Component",
              "props": {
                "content": "$props.info",
                "args": "$props.args"
              }
            }
          ]
        },
        {
          "id": "Condition-3",
          "component": "Condition",
          "props": {
            "resource": "$item",
            "expression": "$isNot"
          },
          "children": [
            {
              "id": "Component-4",
              "component": "Component",
              "props": {
                "content": "$props.edit",
                "args": "$props.args"
              }
            }
          ]
        }
      ]
    }
  ]
}
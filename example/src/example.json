  "functions": {
    "$submit": "console.log(args[0]);"
  },
  "data": [
    {
      "id": "Form-0",
      "component": "Form",
      "props": {
        "type": "inner",
        "onSubmit": "$submit"
      },
      "children": [
        {
          "id": "Div-2",
          "component": "Div",
          "props": {
            "style": {
              "padding": "10px",
              "textAlign": "center"
            }
          },
          "children": [
            {
              "id": "Text-1",
              "component": "Text",
              "props": {
                "content": "$name",
                "style": {
                  "fontSize": "18px",
                  "fontWeight": "bolder"
                }
              }
            }
          ]
        },
        {
          "id": "Div-3",
          "component": "Div",
          "props": {
            "style": {
              "padding": "10px"
            }
          },
          "children": [
            {
              "id": "Text-4",
              "component": "Text",
              "props": {
                "content": "$des"
              }
            },
            {
              "id": "CheckboxGroup-4",
              "component": "CheckboxGroup",
              "props": {
                "name": "scene",
                "label": "1.您想了解的业务场景？（多选）",
                "options": "$sceneList",
                "rule": "REQ"
              }
            },
            {
              "id": "Select-5",
              "component": "Select",
              "props": {
                "name": "scale",
                "label": "2.贵司公司规模？（单选）",
                "options": "$scaleList",
                "rule": "REQ"
              }
            },
            {
              "id": "Select-6",
              "component": "Select",
              "props": {
                "name": "requirements",
                "label": "3.贵司目前的需求阶段？（单选）",
                "options": "$requirementsList",
                "rule": "REQ"
              }
            },
            {
              "id": "Input-7",
              "component": "Input",
              "props": {
                "name": "company",
                "label": "4.公司全称（必填）",
                "rule": "REQ LEN-0-100"
              }
            },
            {
              "id": "Input-8",
              "component": "Input",
              "props": {
                "name": "name",
                "label": "5.您的姓名（必填）",
                "rule": "REQ LEN-0-20"
              }
            },
            {
              "id": "Input-9",
              "component": "Input",
              "props": {
                "name": "phone",
                "label": "6.手机（必填）",
                "rule": "REQ TEL"
              }
            },
            {
              "id": "Input-10",
              "component": "Input",
              "props": {
                "name": "email",
                "label": "7.工作邮箱（必填）",
                "rule": "REQ EMAIL"
              }
            },
            {
              "id": "Row-1",
              "component": "Row",
              "props": {
                "gutter": 4,
                "style": {
                  "width": "100%"
                }
              },
              "children": [
                {
                  "id": "Col-2",
                  "component": "Col",
                  "props": {
                    "flex": 1
                  },
                  "children": [
                    {
                      "id": "SubmitButton-11",
                      "component": "SubmitButton",
                      "props": {
                        "type": "primary",
                        "block": true,
                        "shape": "round",
                        "size": "large"
                      },
                      "children": "提交"
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}
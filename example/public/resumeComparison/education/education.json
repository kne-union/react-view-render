{
  "description": "教育经历模块",
  "components": {
    "$eduInfo": {
      "description": "教育经历list-item",
      "data": [
        {
          "id": "Space-1",
          "component": "Space",
          "props": {
            "direction": "vertical",
            "style": {
              "paddingBottom": "44px",
              "position": "relative"
            }
          },
          "children": [
            {
              "id": "Space-5",
              "component": "Space",
              "props": {
                "direction": "vertical",
                "size": 32,
                "style": {
                  "fontSize": "14px",
                  "fontWeight": 400,
                  "color": "#313753"
                }
              },
              "children": [
                {
                  "id": "Component-0",
                  "component": "Component",
                  "props": {
                    "value": "$detailItem.school",
                    "name": "学校名称：",
                    "content": "$showWorkItem"
                  }
                },
                {
                  "id": "Space-5",
                  "component": "Row",
                  "props": {
                  },
                  "children": [
                    {
                      "id": "Component-1",
                      "component": "Col",
                      "props": {
                        "span": 12
                      },
                      "children": [
                        {
                          "id": "Component-1",
                          "component": "Component",
                          "props": {
                            "value": "$detailItem.degree",
                            "name": "最高学历：",
                            "content": "$showWorkItem"
                          }
                        }
                      ]
                    },
                    {
                      "id": "Component-1",
                      "component": "Col",
                      "props": {
                        "span": 12
                      },
                      "children": [
                        {
                          "id": "Component-1",
                          "component": "Component",
                          "props": {
                            "value": "$detailItem.major",
                            "name": "专业名称：",
                            "content": "$showWorkItem"
                          }
                        }
                      ]
                    }
                  ]
                },

                {
                  "id": "Div-7",
                  "component": "Space",
                  "props": {
                    "direction": "vertical",
                    "size": 16
                  },
                  "children": [
                    {
                      "id": "Text-22",
                      "component": "Text",
                      "props": {
                        "style": {
                          "fontWeight": 400,
                          "color": "#576B95"

{
  "components": {
    "$editInfo": "/react-view-render/resume/editInfo.json",
    "$addForm": "/react-view-render/resume/addForm.json",
    "$resumeCard": "/react-view-render/resume/detailResumeCard.json",
    "$list": {
      "components": {
        "$options": {
          "data": [
            {
              "id": "Space-0",
              "component": "Space",
              "children": [
                {
                  "id": "Button-1",
                  "component": "Button",
                  "props": {
                    "type": "link",
                    "onClick": "$changeEditState"
                  },
                  "children": "编辑"
                },
                {
                  "id": "Button-2",
                  "component": "Button",
                  "props": {
                    "type": "link",
                    "onClick": "$delDetailConfirm"
                  },
                  "children": "删除"
                }
              ]
            }
          ]
        },
        "$form": {
          "data": {
            "id": "Div-0",
            "component": "Div",
            "props": {
              "style": {
                "background": "#F8F8F8",
                "padding": "14px 16px 24px",
                "marginBottom": "20px"
              }
            },
            "children": [
              {
                "id": "Form-1",
                "component": "Form",
                "props": {
                  "data": "$detailItem",
                  "onSubmit": "$editDetail"
                },
                "children": [
                  {
                    "id": "Component-1",
                    "component": "Component",
                    "props": {
                      "content": "$props.args.form"
                    }
                  },
                  {
                    "id": "Row-27",
                    "component": "Row",
                    "props": {
                      "justify": "center",
                      "gutter": [
                        40,
                        100
                      ]
                    },
                    "children": [
                      {
                        "id": "Col-28",
                        "component": "Col",
                        "props": {
                        },
                        "children": [
                          {
                            "id": "SubmitButton-29",
                            "component": "SubmitButton",
                            "props": {
                              "type": "primary"
                            },
                            "children": "保存"
                          }
                        ]
                      },
                      {
                        "id": "Col-30",
                        "component": "Col",
                        "props": {
                        },
                        "children": [
                          {
                            "id": "Button-31",
                            "component": "Button",
                            "props": {
                              "onClick": "$changeInfoState"
                            },
                            "children": "返回"
                          }
                        ]
                      }
                    ]
                  }
                ]
              }
            ]
          }
        },
        "$empty": {
          "data": [
            {
              "id": "Empty-0",
              "component": "Empty"
            }
          ]
        }
      },
      "data": [
        {
          "id": "Space-0",
          "component": "Space",
          "props": {
            "direction": "vertical"
          },
          "children": [
            {
              "id": "Mapping-1",
              "component": "Mapping",
              "props": {
                "expression": "$mapList"
              },
              "children": [
                {
                  "id": "List-3",
                  "component": "List",
                  "props": {
                    "resource": "$item",
                    "empty": "$empty"
                  },
                  "children": [
                    {
                      "id": "Mapping-5",
                      "component": "Mapping",
                      "props": {
                        "resource": "$item",
                        "expression": "$props.itemMapping"
                      },
                      "children": [
                        {
                          "id": "Rename-5",
                          "component": "Rename",
                          "props": {
                            "resource": "$item",
                            "name": "$detailItem"
                          },
                          "children": [
                            {
                              "id": "Component-4",
                              "component": "Component",
                              "props": {
                                "content": "$editInfo",
                                "info": "$props.info",
                                "edit": "$form",
                                "args": {
                                  "optionButton": "$options",
                                  "name": "$props.name",
                                  "form": "$props.form",
                                  "itemMapping": "$props.itemMapping",
                                  "submitMapping": "$props.submitMapping"
                                }
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  },
  "functions": {
    "$mapList": "return variable.$resume[variable.$props.name]",
    "$addDetail": "const current = variable.$resume[variable.$props.name].slice(0);current.push(variable.$props.args.submitMapping(args[0]));functions.$saveResume({[variable.$props.name]:current});",
    "$editDetail": "const current = variable.$resume[variable.$props.args.name].slice(0);current.splice(variable.$index,1,Object.assign({},variable[variable.$props.args.name],variable.$props.args.submitMapping(args[0])));functions.$saveResume({[variable.$props.args.name]:current});functions.$changeInfoState();",
    "$delDetailWork": "const current = variable.$resume[variable.$props.name].slice(0);current.splice(variable.$index,1);functions.$saveResume({[variable.$props.name]:current});",
    "$delDetailConfirm": "functions.$delConfirm(variable.$outerProps.removeTitle,functions.$delDetailWork)"
  },
  "data": [
    {
      "id": "Rename-0",
      "component": "Rename",
      "props": {
        "resource": "$props",
        "name": "$outerProps"
      },
      "children": [
        {
          "id": "Component-10",
          "component": "Component",
          "props": {
            "title": "$outerProps.addTitle",
            "form": "$outerProps.form",
            "data": {},
            "onSubmit": "$addDetail",
            "content": "$addForm"
          },
          "children": [
            {
              "id": "Component-0",
              "component": "Component",
              "props": {
                "content": "$resumeCard",
                "title": "$outerProps.cardTitle",
                "addFunc": "$showAddForm"
              },
              "children": [
                {
                  "id": "Component-1",
                  "component": "Component",
                  "props": {
                    "info": "$outerProps.info",
                    "form": "$outerProps.form",
                    "content": "$list",
                    "name": "$outerProps.name",
                    "itemMapping": "$outerProps.itemMapping",
                    "submitMapping": "$outerProps.submitMapping"
                  }
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}
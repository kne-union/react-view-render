{
  "id":"43",
  "parentId":"41",
  "path":"/resume/index.json",
  "name":"resume的index.json",
  "variable": {
    "$twoList": [
      {
        "label": "是",
        "value": true
      },
      {
        "label": "否",
        "value": false
      }
    ]
  },
  "functions": {
    "$delConfirm": "lib.modal.confirm({content:'确定删除'+args[0]+'?',onOk:args[1]})",
    "$isNot": "return !args[0];",
    "$saveResume": "functions.$saveResumeState(function(old){return Object.assign({},old,args[0]);});lib.message.success('修改成功');"
  },
  "components": {
    "$basic": "/resume/basic.json",
    "$analysis": "/resume/analysis.json",
    "$detail": "/resume/detail.json"
  },
  "data": [
    {
      "id": "State-0",
      "component": "State",
      "props": {
        "initValue": "$initResume",
        "setStateName": "$saveResumeState"
      },
      "children": [
        {
          "id": "Rename-1",
          "component": "Rename",
          "props": {
            "resource": "$item",
            "name": "$resume"
          },
          "children": [
            {
              "id": "Space-3",
              "component": "Space",
              "props": {
                "direction": "vertical"
              },
              "children": [
                {
                  "id": "Component-0",
                  "component": "Component",
                  "props": {
                    "content": "$basic"
                  }
                },
                {
                  "id": "Component-1",
                  "component": "Component",
                  "props": {
                    "content": "$analysis"
                  }
                },
                {
                  "id": "Component-2",
                  "component": "Component",
                  "props": {
                    "content": "$detail"
                  }
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}

      "props": {
        "content": "$card",
        "title": "简历与应聘信息"
      },
      "children": [
        {
          "id": "Tabs-1",
          "component": "Tabs",
          "props": {
            "tabPanes": [
              {
                "key": "0",
                "tab": "标准简历",
                "children": "$standard"
              },
              {
                "key": "1",
                "tab": "简历原件",

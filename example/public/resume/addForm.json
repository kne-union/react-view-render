{
  "id":"47",
  "parentId":"41",
  "path":"/resume/addForm.json",
  "functions": {
    "$showAddForm": "functions.$setModalVisibleState(true);",
    "$hideAddForm": "functions.$setModalVisibleState(false);",
    "$saveAddForm": "variable.$props.onSubmit(...args);functions.$hideAddForm();"
  },
  "data": [
    {
      "id": "State-0",
      "component": "State",
      "props": {
        "initValue": false,
        "setStateName": "$setModalVisibleState"
      },
      "children": [
        {
          "id": "FormModal-1",
          "component": "FormModal",
          "props": {
            "visible": "$item",
            "title": "$props.title",
            "width": "1000px",
            "onCancel": "$hideAddForm",
            "formProps": {
              "type": "inner",
              "data": "$props.data",
              "onSubmit": "$saveAddForm"
            }
          },
          "children": [
            {
              "id": "Component-2",
              "component": "Component",
              "props": {
                "content": "$props.form",
                "className": "form_content",
                "classList":".ant-modal-body{padding:24px 80px} .react-form__field-label{color: #576B95}"
              }
            }
          ]
        },
        {
          "id": "Component-3",
          "component": "Component",
          "props": {
            "content": "$children"
          }
        }
      ]
    }
  ]
}
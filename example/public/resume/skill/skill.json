{
  "description": "技能模块",
  "components": {
    "$skillItemInfo": {
      "description": "技能list-item",
      "data": [
        {
          "id": "listItem1",
          "component": "Space",
          "props": {
            "direction": "vertical",
            "style": {
              "paddingBottom": "10px",
              "position": "relative"
            }
          },
          "children": [
            {
              "id": "listItem2",
              "component": "Space",
              "props": {
                "size": 20
              },
              "children": [
                {
                  "id": "listItem3",
                  "component": "Div",
                  "children": [
                    {
                      "id": "Text-16",
                      "component": "Text",
                      "props": {
                        "style": {
                          "fontWeight": "bold"
                        },
                        "content": "$detailItem.name"
                      }
                    }
                  ]
                },
                {
                  "id": "listItem4",
                  "component": "Div",
                  "prop": {}
                },
                {
                  "id": "listItem5",
                  "component": "Div",
                  "prop": {
                  },
                  "children": [
                    {
                      "id": "listItem8",
                      "component": "Text",
                      "props": {
                        "content": "$detailItem.level"
                      }
                    },
                    {
                      "id": "listItem9",
                      "component": "Text",
                      "props": {
                        "style": {
                          "margin": "0 10px"
                        },
                        "content": "|"
                      }
                    },
                    {
                      "id": "listItem10",
                      "component": "Text",
                      "props": {
                        "content": "$detailItem.month"
                      }
                    }
                  ]
                }

              ]
            },
            {
              "id": "listItem14",
              "component": "Div",
              "props": {
                "style": {
                  "position": "absolute",
                  "right": 0,
                  "top": 0
                }
              },
              "children": [
                {
                  "id": "Component-23",
                  "component": "Component",
                  "props": {
                    "content": "$props.args.optionButton",
                    "name": "$props.args.name"
                  }
                }
              ]
            }
          ]
        }
      ]
    }
  },
  "data": [
    {
      "component": "Component",
      "props": {
        "content": "$skillItemInfo",
        "args": "$props.args"
      }
    }
  ]
}
